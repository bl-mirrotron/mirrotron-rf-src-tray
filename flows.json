    {
        "id": "bd6f804df3d72596",
        "type": "function",
        "z": "b06cee2ed8259749",
        "name": "Scan Watchdog",
        "func": "let staleIntervalmS = flow.get('staleIntervalmS');\nlet llrfPermit = global.get('llrfPermit');\nif ((msg.payload - llrfPermit.timeStamp) > staleIntervalmS) llrfPermit.watchdogChange = false;\nif (!llrfPermit.watchdogChange || (llrfPermit.permit == 0))\n{\n    if (!global.get('settingInProgress'))\n    {\n        let tray = global.get('tray'); \n        if (tray.resetDDS.value == 0)\n        {\n            tray.resetDDS.value = 1;\n            return {topic:'setting', payload:true};\n        }\n    }\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 260,
        "y": 220,
        "wires": [
            [
                "56810d9f9755ecc8"
            ]
        ]
    },
    {
        "id": "361add039165c06f",
        "type": "function",
        "z": "b06cee2ed8259749",
        "name": "Scan machine permit",
        "func": "let llrfPermit = global.get('llrfPermit');\nif (llrfPermit == null) return null;\nif (llrfPermit.watchdog != msg.payload.watchdog.value)\n{\n   llrfPermit.watchdog        =   msg.payload.watchdog.value;\n   llrfPermit.watchdogChange  =   true;\n   llrfPermit.permit          =   msg.payload.permit.value;\n   llrfPermit.timeStamp       =   new Date().getTime();\n}\n\nreturn null;",
        "outputs": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 620,
        "y": 120,
        "wires": []
    },
